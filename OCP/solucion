class Reporte {
  generar() {
    console.log("Reporte gen√©rico");
  }
}

class ReportePerro extends Reporte {
  generar() {
    console.log("Reporte de perro");
  }
}

class ReporteGato extends Reporte {
  generar() {
    console.log("Reporte de gato");
  }
}

const perro = new ReportePerro();
const gato = new ReporteGato();

perro.generar();
gato.generar();